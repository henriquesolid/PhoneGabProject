<?xml version='1.0' encoding='utf-8'?>
<widget id="ie.enclave" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>TravellerHelper</name>
    <description>
        An awesome App for travellers!.
    </description>
    <author email="rricardohenriquesilva@gmail.com" href="https://www.linkedin.com/in/ricardo-silva-dublin/">
        My linkedin
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
